time","original":{"line":119,"column":65},"generated":{"line":1,"column":2285}},{"source":"app.js","name":"displayForecast","original":{"line":58,"column":20},"generated":{"line":1,"column":2293}},{"source":"app.js","name":"f","original":{"line":58,"column":36},"generated":{"line":1,"column":2294}}],"sources":{"app.js":"import {elements} from './el';\r\n\r\nwindow.addEventListener('load', () => {\r\n    let long;\r\n    let lat;\r\n    /*const temperatureDescription = document.querySelector('.temperature-description');\r\n    const temperatureDegree = document.querySelector('.temperature-degree');\r\n    const locationTimezone = document.querySelector('.location-timezone');\r\n    const temperatureSection = document.querySelector('.temperature');\r\n    const temperatureSpan = document.querySelector('.degree-type');\r\n    const timeSection = document.querySelector('.time');*/\r\n\r\n    if(navigator.geolocation){\r\n        navigator.geolocation.getCurrentPosition(position => {\r\n            //console.log(position);\r\n            long = position.coords.longitude;\r\n            lat = position.coords.latitude;\r\n\r\n            const proxy = `https://cors-anywhere.herokuapp.com/`;\r\n            \r\n            const api = `${proxy}https://api.darksky.net/forecast/4345273fe79a1cb9bfbc069cc3179afd/${lat},${long}`;\r\n\r\n            fetch(api)\r\n            .then(response => {\r\n                return response.json();\r\n            })\r\n            .then(data => {\r\n                const {temperature, summary, icon, time} = data.currently;\r\n                const forecast = data.daily.data;\r\n                forecast.shift();\r\n                \r\n                let curDate = new Date();\r\n                //console.log(forecast);\r\n\r\n                elements.timeSection.innerHTML = `${curDate.getHours()} : ${(curDate.getMinutes()<10? '0' : '') + curDate.getMinutes()   }`;\r\n                elements.temperatureDegree.textContent = tempCalc(temperature, 'c');\r\n                /*elements.temperatureDescription.textContent = summary;*/\r\n                elements.locationTimezone.textContent = data.timezone;\r\n\r\n                //formula for celsius\r\n                /*let celsius = Math.floor((temperature - 32)*(5/9));*/\r\n                setIcons(icon, document.querySelector('.icon'));\r\n\r\n                // change temperature to celsius/Farenheit\r\n\r\n                elements.temperatureSection.addEventListener('click', () => {\r\n                    if (elements.temperatureSpan.textContent === \"F\"){\r\n                        elements.temperatureSpan.textContent = \"C\";\r\n                        elements.temperatureDegree.textContent = tempCalc(temperature, 'c');\r\n                    } else {\r\n                        elements.temperatureSpan.textContent = \"F\";\r\n                        elements.temperatureDegree.textContent = tempCalc(temperature, 'f');\r\n                    }\r\n                })\r\n\r\n                \r\n                forecast.forEach(f => {\r\n                    displayForecast(f); \r\n                                                    \r\n                });\r\n\r\n\r\n            });\r\n\r\n        });        \r\n    }\r\n    function tempCalc(temp, format){\r\n        if (format === 'c'){\r\n            return Math.floor((temp - 32)*(5/9));\r\n        } else {\r\n            return Math.floor(temp);\r\n        }\r\n    }\r\n    //const skycons = new Skycons({color: \"white\"});\r\n    function  setIcons(icon, iconID){\r\n        const skycons = new Skycons({color: \"white\"});\r\n        const currentIcon = icon.replace(/-/g,\"_\").toUpperCase();\r\n        skycons.play();\r\n        return skycons.set(iconID, Skycons[currentIcon]);\r\n    }\r\n\r\n    function displayForecast(item){\r\n        //console.log(item);\r\n        // set the days names\r\n        const days = ['Niedziela','Poniedziałek','Wtorek','Środa','Czwartek','Piątek','Sobota'];\r\n        const months = ['Styczeń', \r\n                        'Luty', \r\n                        'Marzec', \r\n                        'Kwiecień', \r\n                        'Maj', \r\n                        'Czerwiec', \r\n                        'Lipiec', \r\n                        'Sierpień', \r\n                        'Wrzesień', \r\n                        'Październik',\r\n                        'Listopad',\r\n                        'Grudzień']\r\n        // create new date object\r\n        let day = new Date();\r\n        // convert unix date object to standard day number\r\n        day.setTime(item.time*1000);\r\n        // set markup\r\n        const markup = `\r\n            <div class=\"forecast-daily\">\r\n                <div class=\"forecast__element forecast__day\">${days[ day.getDay() ]}</div>\r\n                <div class=\"forecast__element forecast__date\">${day.getDate()} ${months[ day.getMonth() ]}</div>\r\n                <div class=\"forecast__element forecase__maxtemp\">${tempCalc(item.temperatureMax, 'c')}<span>&#176 C</span></div>\r\n                \r\n                <div class=\"forecast__element forecast__icon\">\r\n                    <canvas id=\"icon-${item.time}\" width=\"40\" height=\"40\"></canvas>\r\n                </div>\r\n                \r\n            </div>\r\n        `\r\n        const cont = document.querySelector('.forecast')\r\n        //display forecast\r\n        cont.insertAdjacentHTML(\"beforeend\", markup);\r\n        // initiate icons in right containers\r\n        setIcons(item.icon, document.querySelector(`#icon-${item.time}`));\r\n    };\r\n});\r\n\r\n"},"lineCount":null}},"error":null,"hash":"c7d4aa3fb066b450950439afd7c4baa5","cacheData":{"env":{}}}